{
  "name": "@tissuumaps/plugins",
  "version": "0.1.0",
  "type": "module",
  "files": [
    "./dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rm -rf dist node_modules"
  },
  "dependencies": {
    "@tissuumaps/core": "workspace:*"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
