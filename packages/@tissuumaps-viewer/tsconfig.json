{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": ["vite/client"],
    "jsx": "react-jsx"
  },
  "references": [{ "path": "../@tissuumaps-core" }],
  "include": ["./src"],
  "exclude": [
    "./src/**/*.test.js",
    "./src/**/*.test.jsx",
    "./src/**/*.test.ts",
    "./src/**/*.test.tsx"
  ]
}
